        <type>dict</type>
        <definition>dict pygments.lexers.actionscript.MxmlLexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.actionscript.MxmlLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            (&apos;[^&lt;&amp;]+&apos;, Text),
            (r&apos;&amp;\S*?;&apos;, Name.Entity),
            (r&apos;(\&lt;\!\[CDATA\[)(.*?)(\]\]\&gt;)&apos;,
             bygroups(String, using(<ref refid="classpygments_1_1lexers_1_1actionscript_1_1ActionScript3Lexer" kindref="compound">ActionScript3Lexer</ref>), String)),
            (&apos;&lt;!--&apos;, Comment, &apos;comment&apos;),
            (r&apos;&lt;\?.*?\?&gt;&apos;, Comment.Preproc),
            (&apos;&lt;![^&gt;]*&gt;&apos;, Comment.Preproc),
            (r&apos;&lt;\s*[\w:.-]+&apos;, Name.Tag, &apos;tag&apos;),
            (r&apos;&lt;\s*/\s*[\w:.-]+\s*&gt;&apos;, Name.Tag),
        ],
        &apos;comment&apos;: [
            (&apos;[^-]+&apos;, Comment),
            (&apos;--&gt;&apos;, Comment, &apos;#pop&apos;),
            (&apos;-&apos;, Comment),
        ],
        &apos;tag&apos;: [
            (r&apos;\s+&apos;, Whitespace),
            (r&apos;[\w.:-]+\s*=&apos;, Name.Attribute, &apos;attr&apos;),
            (r&apos;/?\s*&gt;&apos;, Name.Tag, &apos;#pop&apos;),
        ],
        &apos;attr&apos;: [
            (r&apos;\s+&apos;, Whitespace),
            (&apos;&quot;.*?&quot;&apos;, String, &apos;#pop&apos;),
            (&quot;&apos;.*?&apos;&quot;, String, &apos;#pop&apos;),
            (r&apos;[^\s&gt;]+&apos;, String, &apos;#pop&apos;),
        ],
    }</initializer>
