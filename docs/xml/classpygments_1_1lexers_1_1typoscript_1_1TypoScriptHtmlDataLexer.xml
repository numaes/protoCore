      <memberdef kind="variable" id="classpygments_1_1lexers_1_1typoscript_1_1TypoScriptHtmlDataLexer_1a08a99d66c7d5bc66997a284d4bcdb2ab" prot="public" static="yes" mutable="no">
        <type>dict</type>
        <definition>dict pygments.lexers.typoscript.TypoScriptHtmlDataLexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.typoscript.TypoScriptHtmlDataLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            # INCLUDE_TYPOSCRIPT
            (r&apos;(INCLUDE_TYPOSCRIPT)&apos;, Name.Class),
            # Language label or extension resource FILE:... or LLL:... or EXT:...
            (r&apos;(EXT|FILE|LLL):[^}\n&quot;]*&apos;, String),
            # marker: ###MARK###
            (r&apos;(.*)(###\w+###)(.*)&apos;, bygroups(String, Name.Constant, String)),
            # constant: {$some.constant}
            (r&apos;(\{)(\$)((?:[\w\-]+\.)*)([\w\-]+)(\})&apos;,
             bygroups(String.Symbol, Operator, Name.Constant,
                      Name.Constant, String.Symbol)),  # constant
            # constant: {register:somevalue}
            (r&apos;(.*)(\{)([\w\-]+)(\s*:\s*)([\w\-]+)(\})(.*)&apos;,
             bygroups(String, String.Symbol, Name.Constant, Operator,
                      Name.Constant, String.Symbol, String)),  # constant
            # whitespace
            (r&apos;\s+&apos;, Text),
            # other
            (r&apos;[&lt;&gt;,:=.*%+|]&apos;, String),
            (r&apos;[\w&quot;\-!/&amp;;(){}#]+&apos;, String),
        ]
    }</initializer>
