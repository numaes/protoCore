        <type>dict</type>
        <definition>dict pygments.lexers.configs.TerminfoLexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.configs.TerminfoLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            (r&apos;^#.*$&apos;, Comment),
            (r&apos;^[^\s#,|]+&apos;, Name.Tag, &apos;names&apos;),
            (r&apos;\s+&apos;, Whitespace),
        ],
        &apos;names&apos;: [
            (r&apos;\n&apos;, Whitespace, &apos;#pop&apos;),
            (r&apos;(,)([ \t]*)&apos;, bygroups(Punctuation, Whitespace), &apos;defs&apos;),
            (r&apos;\|&apos;, Punctuation),
            (r&apos;[^,|]+&apos;, Name.Attribute),
        ],
        &apos;defs&apos;: [
            (r&apos;\n[ \t]+&apos;, Whitespace),
            (r&apos;\n&apos;, Whitespace, &apos;#pop:2&apos;),
            (r&apos;(#)([0-9]+)&apos;, bygroups(Operator, Number)),
            (r&apos;=&apos;, Operator, &apos;data&apos;),
            (r&apos;(,)([ \t]*)&apos;, bygroups(Punctuation, Whitespace)),
            (r&apos;[^\s,=#]+&apos;, Name.Class),
        ],
        &apos;data&apos;: [
            (r&apos;\\[,\\]&apos;, Literal),
            (r&apos;(,)([ \t]*)&apos;, bygroups(Punctuation, Whitespace), &apos;#pop&apos;),
            (r&apos;[^\\,]+&apos;, Literal),  # for performance
            (r&apos;.&apos;, Literal),
        ],
    }</initializer>
