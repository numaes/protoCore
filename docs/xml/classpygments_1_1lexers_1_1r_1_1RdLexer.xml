      <memberdef kind="variable" id="classpygments_1_1lexers_1_1r_1_1RdLexer_1a9feb668af161b8654cbe302a01b03391" prot="public" static="yes" mutable="no">
        <type>dict</type>
        <definition>dict pygments.lexers.r.RdLexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.r.RdLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            # catch escaped brackets and percent sign
            (r&apos;\\[\\{}%]&apos;, String.Escape),
            # comments
            (r&apos;%.*$&apos;, Comment),
            # special macros with no arguments
            (r&apos;\\(?:cr|l?dots|R|tab)\b&apos;, Keyword.Constant),
            # macros
            (r&apos;\\[a-zA-Z]+\b&apos;, Keyword),
            # special preprocessor macros
            (r&apos;^\s*#(?:ifn?def|endif).*\b&apos;, Comment.Preproc),
            # non-escaped brackets
            (r&apos;[{}]&apos;, Name.Builtin),
            # everything else
            (r&apos;[^\\%\n{}]+&apos;, Text),
            (r&apos;.&apos;, Text),
        ]
    }</initializer>
        <briefdescription>
        </briefdescription>
