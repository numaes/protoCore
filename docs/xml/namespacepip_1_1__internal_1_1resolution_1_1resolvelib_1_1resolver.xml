      <memberdef kind="function" id="namespacepip_1_1__internal_1_1resolution_1_1resolvelib_1_1resolver_1a6f976f8bb9a366641d4e51f7e2ad93b1" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
