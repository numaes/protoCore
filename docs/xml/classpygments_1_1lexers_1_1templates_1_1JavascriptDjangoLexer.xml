      <memberdef kind="function" id="classpygments_1_1lexers_1_1templates_1_1JavascriptDjangoLexer_1aea9ff6263331039b6a6e74e230806317" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
