        <argsstring>(self, request, verify, cert=None)</argsstring>
        <name>build_connection_pool_key_attributes</name>
        <qualifiedname>requests.adapters.HTTPAdapter.build_connection_pool_key_attributes</qualifiedname>
        <param>
          <type>self</type>
