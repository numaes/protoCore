        <type>dict</type>
        <definition>dict pygments.lexers.configs.RegeditLexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.configs.RegeditLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            (r&apos;Windows Registry Editor.*&apos;, Text),
            (r&apos;\s+&apos;, Whitespace),
            (r&apos;[;#].*&apos;, Comment.Single),
            (r&apos;(\[)(-?)(HKEY_[A-Z_]+)(.*?\])$&apos;,
             bygroups(Keyword, Operator, Name.Builtin, Keyword)),
            # String keys, which obey somewhat normal escaping
            (r&apos;(&quot;(?:\\&quot;|\\\\|[^&quot;])+&quot;)([ \t]*)(=)([ \t]*)&apos;,
             bygroups(Name.Attribute, Whitespace, Operator, Whitespace),
             &apos;value&apos;),
            # Bare keys (includes @)
            (r&apos;(.*?)([ \t]*)(=)([ \t]*)&apos;,
             bygroups(Name.Attribute, Whitespace, Operator, Whitespace),
             &apos;value&apos;),
        ],
        &apos;value&apos;: [
            (r&apos;-&apos;, Operator, &apos;#pop&apos;),  # delete value
            (r&apos;(dword|hex(?:\([0-9a-fA-F]\))?)(:)([0-9a-fA-F,]+)&apos;,
             bygroups(Name.Variable, Punctuation, Number), &apos;#pop&apos;),
            # As far as I know, .reg files do not support line continuation.
            (r&apos;.+&apos;, String, &apos;#pop&apos;),
            <ref refid="classpygments_1_1lexer_1_1default" kindref="compound">default</ref>(&apos;#pop&apos;),
        ]
    }</initializer>
