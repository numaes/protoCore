      <memberdef kind="function" id="classpygments_1_1lexers_1_1c__cpp_1_1CLexer_1a56eafba9ed0e40de3e0a4ba74e2fa162" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
