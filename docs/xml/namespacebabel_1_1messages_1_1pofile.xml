          <declname>fileobj</declname>
          <array>[AnyStr][AnyStr]</array>
        </param>
        <param>
          <type><ref refid="classbabel_1_1core_1_1Locale" kindref="compound">Locale</ref>|str|None</type>
          <declname>locale</declname>
          <defval>None</defval>
        </param>
