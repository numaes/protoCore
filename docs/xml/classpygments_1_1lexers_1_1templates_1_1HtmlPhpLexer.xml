      <memberdef kind="function" id="classpygments_1_1lexers_1_1templates_1_1HtmlPhpLexer_1a60600a0793ef4cd7444de616b6194242" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
