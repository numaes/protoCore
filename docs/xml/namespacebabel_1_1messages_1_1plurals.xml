        <argsstring>(Locale|str|None locale=None)</argsstring>
        <name>get_plural</name>
        <qualifiedname>babel.messages.plurals.get_plural</qualifiedname>
        <param>
          <type><ref refid="classbabel_1_1core_1_1Locale" kindref="compound">Locale</ref>|str|None</type>
