# Simple Doxyfile for the Proto project

PROJECT_NAME      = "Proto"
INPUT             = ../headers
OUTPUT_DIRECTORY  = .
GENERATE_XML      = YES
GENERATE_HTML     = NO
GENERATE_LATEX    = NO
XML_OUTPUT        = xml
RECURSIVE         = YES
QUIET             = YES
EXTRACT_ALL       = YES
EXTRACT_PRIVATE   = NO
EXTRACT_STATIC    = YES
EXCLUDE           = ../build* ../.vscode ../test
