        &apos;root&apos;: [
            (r&apos;\s+&apos;, Whitespace),
            (r&apos;--.*\n?&apos;, Comment.Single),
            (r&apos;/\*&apos;, Comment.Multiline, &apos;multiline-comments&apos;),
