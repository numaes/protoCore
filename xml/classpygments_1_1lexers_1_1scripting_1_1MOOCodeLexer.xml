      <memberdef kind="variable" id="classpygments_1_1lexers_1_1scripting_1_1MOOCodeLexer_1ac94fca23378823769f73fc9834c2ee92" prot="public" static="yes" mutable="no">
        <type>dict</type>
        <definition>dict pygments.lexers.scripting.MOOCodeLexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.scripting.MOOCodeLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            # Numbers
            (r&apos;(0|[1-9][0-9_]*)&apos;, Number.Integer),
            # Strings
            (r&apos;&quot;(\\\\|\\[^\\]|[^&quot;\\])*&quot;&apos;, String),
            # exceptions
            (r&apos;(E_PERM|E_DIV)&apos;, Name.Exception),
            # db-refs
            (r&apos;((#[-0-9]+)|(\$\w+))&apos;, Name.Entity),
            # Keywords
            (r&apos;\b(if|else|elseif|endif|for|endfor|fork|endfork|while&apos;
             r&apos;|endwhile|break|continue|return|try&apos;
             r&apos;|except|endtry|finally|in)\b&apos;, Keyword),
            # builtins
            (r&apos;(random|length)&apos;, Name.Builtin),
            # special variables
            (r&apos;(player|caller|this|args)&apos;, Name.Variable.Instance),
            # skip whitespace
            (r&apos;\s+&apos;, Text),
            (r&apos;\n&apos;, Text),
            # other operators
            (r&apos;([!;=,{}&amp;|:.\[\]@()&lt;&gt;?]+)&apos;, Operator),
            # function call
            (r&apos;(\w+)(\()&apos;, bygroups(Name.Function, Operator)),
            # variables
            (r&apos;(\w+)&apos;, Text),
        ]
    }</initializer>
