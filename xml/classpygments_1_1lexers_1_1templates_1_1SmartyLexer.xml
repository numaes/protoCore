      <memberdef kind="function" id="classpygments_1_1lexers_1_1templates_1_1SmartyLexer_1add76221075c0053835a378ce19858705" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type></type>
