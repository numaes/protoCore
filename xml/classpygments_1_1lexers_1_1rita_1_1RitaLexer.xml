            (r&apos;([a-z0-9_]+)&apos;, Name),
            (r&apos;((-&gt;)|[!?+*|=])&apos;, Operator),
            (r&apos;[\(\),\{\}]&apos;, Punctuation)
        ]
    }</initializer>
