        &apos;root&apos;: [
            (r&apos;\n&apos;, Whitespace),
            (r&apos;\s+&apos;, Whitespace),
            (r&apos;#(.*?)\n&apos;, Comment.Single),
            (r&apos;@(.*?)\n&apos;, Operator),  # Yes, whole line as an operator
            (r&apos;&quot;(\w|\d|\s|(\\&quot;)|[\&apos;_\-./,\?\!])+?&quot;&apos;, Literal),
