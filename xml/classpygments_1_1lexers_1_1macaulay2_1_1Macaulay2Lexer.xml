      <memberdef kind="variable" id="classpygments_1_1lexers_1_1macaulay2_1_1Macaulay2Lexer_1a1251d868e355cd30dcd1284557bfce34" prot="public" static="yes" mutable="no">
        <type>dict</type>
        <definition>dict pygments.lexers.macaulay2.Macaulay2Lexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.macaulay2.Macaulay2Lexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            (r&apos;--.*$&apos;, Comment.Single),
            (r&apos;-\*&apos;, Comment.Multiline, &apos;block comment&apos;),
            (r&apos;&quot;&apos;, String, &apos;quote string&apos;),
            (r&apos;///&apos;, String, &apos;slash string&apos;),
            (<ref refid="classpygments_1_1lexer_1_1words" kindref="compound">words</ref>(<ref refid="namespacepygments_1_1lexers_1_1macaulay2_1ac6e1026eeaf7724dc1bcdbe160fe9784" kindref="member">M2KEYWORDS</ref>, prefix=r&apos;\b&apos;, suffix=r&apos;\b&apos;), Keyword),
            (<ref refid="classpygments_1_1lexer_1_1words" kindref="compound">words</ref>(<ref refid="namespacepygments_1_1lexers_1_1macaulay2_1aff2f97a2d13668691e72e8fef9f261c8" kindref="member">M2DATATYPES</ref>, prefix=r&apos;\b&apos;, suffix=r&apos;\b&apos;), Name.Builtin),
            (<ref refid="classpygments_1_1lexer_1_1words" kindref="compound">words</ref>(<ref refid="namespacepygments_1_1lexers_1_1macaulay2_1ac6913b5eeb8a396e8cc505cfc72769cf" kindref="member">M2FUNCTIONS</ref>, prefix=r&apos;\b&apos;, suffix=r&apos;\b&apos;), Name.Function),
            (<ref refid="classpygments_1_1lexer_1_1words" kindref="compound">words</ref>(<ref refid="namespacepygments_1_1lexers_1_1macaulay2_1a3c6195514b08fc280858a1ca1d8aecb3" kindref="member">M2CONSTANTS</ref>, prefix=r&apos;\b&apos;, suffix=r&apos;\b&apos;), Name.Constant),
            (r&apos;\s+&apos;, Text.Whitespace),
            (r&apos;.&apos;, Text)
        ],
        &apos;block comment&apos; : [
            (r&apos;[^*-]+&apos;, Comment.Multiline),
            (r&apos;\*-&apos;, Comment.Multiline, &apos;#pop&apos;),
            (r&apos;[*-]&apos;, Comment.Multiline)
        ],
        &apos;quote string&apos; : [
            (r&apos;[^\\&quot;]+&apos;, String),
            (r&apos;&quot;&apos;, String, &apos;#pop&apos;),
            (r&apos;\\&quot;?&apos;, String),
        ],
        &apos;slash string&apos; : [
            (r&apos;[^/]+&apos;, String),
            (r&apos;(//)+(?!/)&apos;, String),
            (r&apos;/(//)+(?!/)&apos;, String, &apos;#pop&apos;),
            (r&apos;/&apos;, String)
        ]
    }</initializer>
