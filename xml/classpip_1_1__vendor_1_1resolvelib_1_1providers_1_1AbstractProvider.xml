        <definition>pip._vendor.resolvelib.providers.AbstractProvider.get_preference</definition>
        <argsstring>(self, identifier, resolutions, candidates, information, backtrack_causes)</argsstring>
        <name>get_preference</name>
        <qualifiedname>pip._vendor.resolvelib.providers.AbstractProvider.get_preference</qualifiedname>
        <param>
          <type>self</type>
          <defname>self</defname>
        </param>
        <param>
          <type>identifier</type>
          <defname>identifier</defname>
        </param>
        <param>
          <type>resolutions</type>
