        &apos;root&apos;: [
            (r&apos;^#&apos;, Comment.Preproc, (&apos;expr&apos;, &apos;exprstart&apos;)),
            (r&apos;.+&apos;, Other),
        ],
        &apos;exprstart&apos;: [
            (r&apos;(literal)(.*)&apos;, bygroups(Comment.Preproc, Text), &apos;#pop:2&apos;),
            (<ref refid="classpygments_1_1lexer_1_1words" kindref="compound">words</ref>((
                &apos;define&apos;, &apos;undef&apos;, &apos;if&apos;, &apos;ifdef&apos;, &apos;ifndef&apos;, &apos;else&apos;, &apos;elif&apos;,
