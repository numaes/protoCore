                &apos;<ref refid="classpygments_1_1lexer_1_1include" kindref="compound">include</ref>&apos;, &apos;includesubst&apos;, &apos;error&apos;)),
             Comment.Preproc, &apos;#pop&apos;),
        ],
        &apos;expr&apos;: [
            (<ref refid="classpygments_1_1lexer_1_1words" kindref="compound">words</ref>((&apos;!&apos;, &apos;!=&apos;, &apos;==&apos;, &apos;&amp;&amp;&apos;, &apos;||&apos;)), Operator),
            (r&apos;(defined)(\()&apos;, bygroups(Keyword, Punctuation)),
            (r&apos;\)&apos;, Punctuation),
            (r&apos;[0-9]+&apos;, Number.Decimal),
            (r&apos;__\w+?__&apos;, Name.Variable),
            (r&apos;@\w+?@&apos;, Name.Class),
            (r&apos;\w+&apos;, Name),
            (r&apos;\n&apos;, Text, &apos;#pop&apos;),
            (r&apos;\s+&apos;, Text),
            (r&apos;\S&apos;, Punctuation),
        ],
    }</initializer>
