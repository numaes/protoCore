      <memberdef kind="variable" id="classpygments_1_1lexers_1_1configs_1_1IniLexer_1ad588fb4fa41a0d68a6570004a70c193a" prot="public" static="yes" mutable="no">
        <type>dict</type>
        <definition>dict pygments.lexers.configs.IniLexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.configs.IniLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            (r&apos;\s+&apos;, Whitespace),
            (r&apos;[;#].*&apos;, Comment.Single),
            (r&apos;(\[.*?\])([ \t]*)$&apos;, bygroups(Keyword, Whitespace)),
            (,
             bygroups(Name.Attribute, Whitespace, Operator, Whitespace, String),
             &quot;quoted_value&quot;),
            (r&apos;(.*?)([ \t]*)([=:])([ \t]*)([^;#\n]*)(\\)(\s+)&apos;,
             bygroups(Name.Attribute, Whitespace, Operator, Whitespace, String,
                      Text, Whitespace),
             &quot;value&quot;),
            (r&apos;(.*?)([ \t]*)([=:])([ \t]*)([^ ;#\n]*(?: +[^ ;#\n]+)*)&apos;,
             bygroups(Name.Attribute, Whitespace, Operator, Whitespace, String)),
            # standalone option, supported by some INI parsers
            (r&apos;(.+?)$&apos;, Name.Attribute),
        ],
        &apos;quoted_value&apos;: [
            (,
             bygroups(String, String, Whitespace), &quot;#pop&quot;),
            (r&apos;[;#].*&apos;, Comment.Single),
            (r&apos;$&apos;, String, &quot;#pop&quot;),
        ],
        &apos;<ref refid="build_2__deps_2googletest-src_2googlemock_2test_2gmock-actions__test_8cc_1ac4f474c82e82cbb89ca7c36dd52be0ed" kindref="member">value</ref>&apos;: [     # line continuation
            (r&apos;\s+&apos;, Whitespace),
            (r&apos;(\s*)(.*)(\\)([ \t]*)&apos;,
             bygroups(Whitespace, String, Text, Whitespace)),
            (r&apos;.*$&apos;, String, &quot;#pop&quot;),
        ],
    }</initializer>
