        <initializer>=  {
        &apos;root&apos;: [
            # Whitespace
            (r&apos;\s+&apos;, Whitespace),

            # single line comment
            (r&apos;(//.*?)(\n)&apos;, bygroups(Comment.Single, Whitespace)),

            # lid header
            (r&apos;(.*?)(:)([ \t]*)(.*(?:\n[ \t].+)*)&apos;,
             bygroups(Name.Attribute, Operator, Whitespace, String)),
        ]
    }</initializer>
