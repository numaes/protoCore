        <type>dict</type>
        <definition>dict pygments.lexers.markup.BBCodeLexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.markup.BBCodeLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            (r&apos;[^[]+&apos;, Text),
            # tag/end tag begin
            (r&apos;\[/?\w+&apos;, Keyword, &apos;tag&apos;),
            # stray bracket
            (r&apos;\[&apos;, Text),
        ],
        &apos;tag&apos;: [
            (r&apos;\s+&apos;, Text),
            # attribute with <ref refid="build_2__deps_2googletest-src_2googlemock_2test_2gmock-actions__test_8cc_1ac4f474c82e82cbb89ca7c36dd52be0ed" kindref="member">value</ref>
            (r&apos;(\w+)(=)(&quot;?[^\s&quot;\]]+&quot;?)&apos;,
             bygroups(Name.Attribute, Operator, String)),
            # tag argument (a la [color=green])
            (r&apos;(=)(&quot;?[^\s&quot;\]]+&quot;?)&apos;,
             bygroups(Operator, String)),
            # tag end
            (r&apos;\]&apos;, Keyword, &apos;#pop&apos;),
        ],
    }</initializer>
