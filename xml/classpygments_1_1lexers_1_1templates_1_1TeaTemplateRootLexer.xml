        <name>tokens</name>
        <qualifiedname>pygments.lexers.templates.TeaTemplateRootLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            (r&apos;&lt;%\S?&apos;, Keyword, &apos;sec&apos;),
            (r&apos;[^&lt;]+&apos;, Other),
            (r&apos;&lt;&apos;, Other),
        ],
        &apos;sec&apos;: [
            (r&apos;%&gt;&apos;, Keyword, &apos;#pop&apos;),
            # note: &apos;\w\W&apos; != &apos;.&apos; without DOTALL.
            (r&apos;[\w\W]+?(?=%&gt;|\Z)&apos;, using(<ref refid="classpygments_1_1lexers_1_1jvm_1_1TeaLangLexer" kindref="compound">TeaLangLexer</ref>)),
        ],
    }</initializer>
