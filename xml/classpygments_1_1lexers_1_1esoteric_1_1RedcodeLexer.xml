        <type>dict</type>
        <definition>dict pygments.lexers.esoteric.RedcodeLexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.esoteric.RedcodeLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            # Whitespace:
            (r&apos;\s+&apos;, Whitespace),
            (r&apos;;.*$&apos;, Comment.Single),
            # Lexemes:
            #  Identifiers
            (r&apos;\b({})\b&apos;.<ref refid="namespacepygments_1a2ac77ff8d2ce68f7e1ee8c8933712687" kindref="member">format</ref>(&apos;|&apos;.join(<ref refid="classpygments_1_1lexers_1_1esoteric_1_1RedcodeLexer_1a08208e2737a8c9f5d13b6cab1ccedb5f" kindref="member">opcodes</ref>)), Name.Function),
            (r&apos;\b({})\b&apos;.<ref refid="namespacepygments_1a2ac77ff8d2ce68f7e1ee8c8933712687" kindref="member">format</ref>(&apos;|&apos;.join(<ref refid="classpygments_1_1lexers_1_1esoteric_1_1RedcodeLexer_1a5af75a040389249a11c2451066784ccd" kindref="member">modifiers</ref>)), Name.Decorator),
            (r&apos;[A-Za-z_]\w+&apos;, Name),
            #  Operators
            (r&apos;[-+*/%]&apos;, Operator),
            (r&apos;[#$@&lt;&gt;]&apos;, Operator),  # mode
            (r&apos;[.,]&apos;, Punctuation),  # mode
            #  Numbers
            (r&apos;[-+]?\d+&apos;, Number.Integer),
        ],
    }</initializer>
