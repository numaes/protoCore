      <memberdef kind="variable" id="classpygments_1_1lexers_1_1textfmts_1_1IrcLogsLexer_1ad189e7b24bb569c718c887807d7e38b0" prot="public" static="yes" mutable="no">
        <type>dict</type>
        <definition>dict pygments.lexers.textfmts.IrcLogsLexer::tokens</definition>
        <argsstring></argsstring>
        <name>tokens</name>
        <qualifiedname>pygments.lexers.textfmts.IrcLogsLexer.tokens</qualifiedname>
        <initializer>=  {
        &apos;root&apos;: [
            # log start/end
            (r&apos;^\*\*\*\*(.*)\*\*\*\*$&apos;, Comment),
            # hack
            (&quot;^&quot; + <ref refid="classpygments_1_1lexers_1_1textfmts_1_1IrcLogsLexer_1a98104d8313036aff04006a2c0b8038b3" kindref="member">timestamp</ref> + r&apos;(\s*&lt;[^&gt;]*&gt;\s*)$&apos;, bygroups(Comment.Preproc, Name.Tag)),
            # normal msgs
            (&quot;^&quot; + <ref refid="classpygments_1_1lexers_1_1textfmts_1_1IrcLogsLexer_1a98104d8313036aff04006a2c0b8038b3" kindref="member">timestamp</ref> + ,
             bygroups(Comment.Preproc, Name.Tag), &apos;msg&apos;),
            # /me msgs
            (&quot;^&quot; + <ref refid="classpygments_1_1lexers_1_1textfmts_1_1IrcLogsLexer_1a98104d8313036aff04006a2c0b8038b3" kindref="member">timestamp</ref> + ,
             bygroups(Comment.Preproc, Keyword, Generic.Inserted)),
            # join/part msgs
            (&quot;^&quot; + <ref refid="classpygments_1_1lexers_1_1textfmts_1_1IrcLogsLexer_1a98104d8313036aff04006a2c0b8038b3" kindref="member">timestamp</ref> + ,
             bygroups(Comment.Preproc, Keyword, String, Comment)),
            (r&quot;^.*?\n&quot;, Text),
        ],
        &apos;msg&apos;: [
            (r&quot;\S+:(?!//)&quot;, Name.Attribute),  # Prefix
            (r&quot;.*\n&quot;, Text, &apos;#pop&apos;),
        ],
    }</initializer>
