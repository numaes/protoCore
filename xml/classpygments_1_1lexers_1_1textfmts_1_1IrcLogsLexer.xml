      <memberdef kind="variable" id="classpygments_1_1lexers_1_1textfmts_1_1IrcLogsLexer_1a98104d8313036aff04006a2c0b8038b3" prot="public" static="yes" mutable="no">
        <type>str</type>
        <definition>str pygments.lexers.textfmts.IrcLogsLexer::timestamp</definition>
        <argsstring></argsstring>
        <name>timestamp</name>
        <qualifiedname>pygments.lexers.textfmts.IrcLogsLexer.timestamp</qualifiedname>
        <initializer>=  r&quot;&quot;&quot;
        (
          # irssi / xchat and others
          (?: \[|\()?                  # Opening bracket or paren for the timestamp
            (?:                        # Timestamp
                (?: (?:\d{1,4} [-/])*  # Date as - or /-separated groups of digits
                    (?:\d{1,4})
                 [T ])?                # Date/time separator: T or space
                (?: \d?\d [:.])*       # Time as :/.-separated groups of 1 or 2 digits
                    (?: \d?\d)
            )
          (?: \]|\))?\s+               # Closing bracket or paren for the timestamp
        |
          # weechat
          \d{4}\s\w{3}\s\d{2}\s        # Date
          \d{2}:\d{2}:\d{2}\s+         # Time + Whitespace
        |
          # xchat
          \w{3}\s\d{2}\s               # Date
          \d{2}:\d{2}:\d{2}\s+         # Time + Whitespace
        )?
    &quot;&quot;&quot;</initializer>
        <briefdescription>
        </briefdescription>
        <detaileddescription>
        </detaileddescription>
        <inbodydescription>
        </inbodydescription>
        <location file="docs/venv/lib/python3.12/site-packages/pygments/lexers/textfmts.py" line="36" column="1" bodyfile="docs/venv/lib/python3.12/site-packages/pygments/lexers/textfmts.py" bodystart="36" bodyend="-1"/>
