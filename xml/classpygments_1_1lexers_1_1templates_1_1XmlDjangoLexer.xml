      <memberdef kind="function" id="classpygments_1_1lexers_1_1templates_1_1XmlDjangoLexer_1ad237dfb175c35cc8e9fe9edcea623058" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
